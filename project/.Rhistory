geom_point(data = x, aes(facility_longitude, facility_latitude, colour  = total_deaths, size = 1.5), na.rm= T) + #WE just add the chart in without using ggplot!
labs(x= "Longitude", y= "Latitude", title="Cardiac Locations")
})
